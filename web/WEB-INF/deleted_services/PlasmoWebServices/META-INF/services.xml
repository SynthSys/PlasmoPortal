<service name="PlasmoWebServices" scope="application" targetNamespace="http://www.plasmo.ed.ac.uk/">
    <description>
        Plasmo Web Service
    </description>
    <messageReceivers>
        <messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-only"
                         class="org.apache.axis2.rpc.receivers.RPCInOnlyMessageReceiver"/>
        <messageReceiver mep="http://www.w3.org/2004/08/wsdl/in-out"
                         class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
    </messageReceivers>
    <schema schemaNamespace="http://www.plasmo.ed.ac.uk/xsd"/>
    <parameter name="ServiceClass">uk.ac.ed.plasmo.service.PlasmoWebServices</parameter>
</service>